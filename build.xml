<project name="extDBus">
	<dirname property="extDBus.basedir" file="${ant.file}" />
	<property file="${extDBus.basedir}/_extras/build.properties" />

	<target name="compile.phpBuilder" description="Compile module ${ant.project.name}">
		<exec executable="python">
			<arg file="${phpBuilder.filePathName}" />
			<arg value="--filetype=${phpBuilder.fileTypes}" />
			<arg value="--include=${extDBus.basedir}/dNG" />
			<arg value="--strip_prefix=${extDBus.basedir}/" />
		</exec>
	</target>
</project>